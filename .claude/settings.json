{
  "allowed_commands": [
    "git add",
    "git commit",
    "git push",
    "git pull",
    "git status",
    "git diff",
    "git log",
    "git branch",
    "git checkout",
    "git merge",
    "git reset",
    "git stash",
    "npm install",
    "npm run dev",
    "npm run build",
    "npm test",
    "npm run lint",
    "npm run format",
    "npx",
    "bun install",
    "bun run dev",
    "bun test",
    "supabase db push",
    "supabase functions deploy",
    "supabase gen types",
    "supabase db diff",
    "supabase db reset",
    "cat",
    "ls",
    "cd",
    "pwd",
    "mkdir",
    "touch",
    "rm",
    "cp",
    "mv",
    "echo",
    "grep",
    "find",
    "sed",
    "awk",
    "head",
    "tail",
    "wc",
    "sort",
    "uniq",
    "diff",
    "tree",
    "which",
    "node",
    "tsx",
    "ts-node"
  ],
  "denied_commands": [
    "rm -rf /",
    "rm -rf ~",
    "sudo rm -rf",
    "chmod 777 /",
    ":(){ :|:& };:",
    "mkfs",
    "dd if=/dev/zero",
    "> /dev/sda"
  ],
  "auto_approve_patterns": [
    "*"
  ],
  "require_approval_patterns": [
    "rm -rf /*",
    "git push --force",
    "npm publish",
    "*.env*",
    "supabase db reset --force"
  ],
  "environment": {
    "NODE_ENV": "development",
    "EDITOR": "code"
  }
} 